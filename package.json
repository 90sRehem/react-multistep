{
  "name": "react-multistep",
  "version": "5.0.3",
  "description": "Responsive ReactJS multistep form component",
  "main": "index.umd.js",
  "module": "index.esm.js",
  "license": "MIT",
  "author": {
    "name": "@djidja8",
    "email": "srdjan@timok.com",
    "url": "https://github.com/srdjan"
  },
  "keywords": [
    "react, reactjs, responsive, component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/srdjan/react-multistep"
  },
  "homepage": "https://github.com/srdjan/react-multistep",
  "bugs": {
    "url": "https://github.com/srdjan/react-multistep/issues"
  },
  "dependencies": {
    "goober": "1.8.0"
  },
  "devDependencies": {
    "csstype": "^2.6.10",
    "esbuild": "^0.1.16"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "scripts": {
    "build": "esbuild react-multistep.js '--define:process.env.NODE_ENV=\"development\"' --bundle --format=cjs --loader:.js=jsx --outfile=index.js",
    "build:prod": "esbuild react-multistep.js '--define:process.env.NODE_ENV=\"production\"' --bundle --minify --format=cjs --loader:.js=jsx --outfile=index.js",
    "pub:dry": "npm publish ./ --dry-run",
    "pub": "npm publish ./"
  }
}
